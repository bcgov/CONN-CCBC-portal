    "nodemon": "^2.0.20",
    "prettier": "^2.6.2",
    "react-dom": "^18.2.0",
    "relay-compiler": "^13.2.0",
    "relay-test-utils": "^13.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.9.1",
    "typescript": "4.6.3",
    "wait-on": "^6.0.1"
  },
  "resolutions": {
    "@sideway/formula": "3.0.1",
    "json5": "2.2.3",
    "jsonwebtoken": "9.0.0",
    "underscore": "1.12.1"
  },
  "relay": {
    "src": "./",
    "schema": "./schema/schema.graphql",
    "language": "typescript",
    "artifactDirectory": "./__generated__",
    "exclude": [
      "**/.next/**",
      "**/node_modules/**",
      "**/__generated__/**",
      "**/server/**",
      "**/.persisted_operations/**"
    ],
    "persistConfig": {
      "file": "./schema/queryMap.json"
    }
  },
  "graphql": {
    "schema": "./schema/schema.graphql"
  }
}
